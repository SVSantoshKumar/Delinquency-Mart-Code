	TRUNCATE TABLE LOAD_DELIQUENCY_MART
	INSERT INTO LOAD_DELIQUENCY_MART( 
	LOAN_NO,
--	LOAN_BRANCH,
--	SCHEME,
	CUSTOMER_ID,
	UNIQUE_CUSTOMER,
	UNIQUE_CUSTOMER_FLAG_BUCKET,
--	TOTAL_NO_OF_INSTALLMENT,
	MORATORIUM,
--	TOTAL_BILLED_INSTALLMENT_NO,
--	TOTAL_RECEIV_INSTALLMENT,
	REMAINING_TENURE,
--	OVERDUE_INSTALLMENT_COUNT,
--	OVERDUE_PRE_EMI_COUNT,
	GROSS_BOUNCE,
	NET_BOUNCE,
	GROSS_AMOUNT,
--	TOTAL_DUE,
--	LPP,
--	PRE_EMI,
--	CBC_DUE,
--	OTHER_CHARGES_DUE,
	PENAL_AMOUNT_OUTSTANDING,
	NPA_LOAN,
	TOTAL_POS,
	CR,
	DPD,
	OP_DPD,
	MAR_XX_DPD,
	COLLX_BKT,
	BUCKET_HEAD,
	CYCLE,
	CURRENT_DPD_SCALE,
	OP_DPD_SCALE,
	CURRENT_STATUS,
	STATUS,
	PAID_STATUS,	
	RF_CASES,
	FILE_EXTRACT_DATE,
	BUSINESS_DATE,
--	CURRENT_MONTH_INSTALLMENT,
--	CURRENT_MONTH_INSTALLMENT_DUE_DATE,
--	LOAN_AMOUNT,
--	LAST_RECEIVED_DATE,
--	LAST_RECEIPT_AMOUNT,
--	FIRST_EMI_DATE,
--	LAST_EMI_DATE,
	CYCLE_DATE,
	ON_OFF_BOOK,
--	REPO_FLAG,
--	OVERDUE_PRINCIPAL,
--	OVERDUE_INTEREST,
	EMI_PATTERN,
--	BOOKING_DATE,
--	CHEQUE_IN_TRANSIT_AMT,
--	CHEQUE_IN_TRANSIT_COUNT,
	ASSET_COLLATERAL_VALUE,
	DISBURSAL_STATUS,
	LOAN_STATUS,
--	LOAN_PRODUCT,
--	LOAN_EFFECTIVE_RATE,
	EMI_PEMI,
	INSTALLMENT_MODE,
--	LOAN_SANCTION_DATE,
--	LAST_DISBURMENT_DATE,
	NET_LTV,
--	NPA_STAGEID,
--	EMI_FREQUENCY,
	EMI_OVERDUE_AMOUNT,
	ROID,	
--	ALLOCATION,
--	ALLOCATION_DATE,
--	DAY1_ALLOCATION,
--	ALLOCATION_CHANNEL,
--	AGENCY_CODE,
--	NPA_STAGEID_NEW 
)
	SELECT 
		TRIM(CAST(cd.LOAN_NO AS VARCHAR)),
		--TRIM(cd.LOAN_BRANCH),
		--TRIM(cd.SCHEME),
		TRIM(lad.CUST_ID_FLEX),
 		 CASE 
      		 WHEN cust_loan_check.loan_count = 1 THEN '1'
      		 ELSE '0'
    		 END AS UNIQUE_CUSTOMER,
		 CASE 
		  WHEN MAX(cd.DPD) = 0 THEN 'a:0-0'
		  WHEN MAX(cd.DPD) >= 1 AND MAX(cd.DPD) <= 30 THEN 'b:1-30'
		  WHEN MAX(cd.DPD) >= 31 AND MAX(cd.DPD) <= 60 THEN 'c:31-60'
		  WHEN MAX(cd.DPD) >= 61 AND MAX(cd.DPD) <= 90 THEN 'd:61-90'
		  WHEN MAX(cd.DPD) >= 91 AND MAX(cd.DPD) <= 120 THEN 'e:91-120'
		  WHEN MAX(cd.DPD) >= 121 AND MAX(cd.DPD) <= 180 THEN 'f:121-180'
		  WHEN MAX(cd.DPD) > 180 THEN 'g>180'
		END AS UNIQUE_CUSTOMER_FLAG_BUCKET,
		--TRIM(cd.TOTAL_NO_OF_INSTL),
		TRIM(cd.MORATORIUM),
		--TRIM(cd.TOTAL_BILLED_INSTL_NO),
		--TRIM(cd.TOTAL_RECEIV_INSTL),
		(cd.TOTAL_NO_OF_INSTL - cd.TOTAL_BILLED_INSTL_NO) AS REMAINING_TENURE,
		--cd.OVERDUE_INSTL_COUNT,
		--cd.OVERDUE_PRE_EMI_COUNT,
		NULL AS GROSS_BOUNCE,
		NULL AS NET_BOUNCE,		
		NULL AS ALLOCATEDAMOUNT,
		--cd.TOTAL_DUE,
		--cd.LPP,
		--cd.PRE_EMI,
		--cd.CBC_DUE,
		--cd.OTHER_CHARGES_DUE,
		(cd.LPP + cd.PRE_EMI + cd.CBC_DUE + cd.OTHER_CHARGES_DUE) AS PENAL_AMOUNT_OUTSTANDING,
		CASE 
		WHEN laad.FLG_ACCR_STATUS = 'S' then 'NPA'
		ELSE 'NORMAL' end as NPA_LOAN,
		cd.TOTAL_POS,
		(cd.TOTAL_POS/10000000) AS CR,
		cd.DPD,
		ld.OP_DPD AS OP_DPD,
		march_dpd.MARCH_XX_DPD,
		CASE 
		  WHEN ld.OP_DPD = 0 THEN 'a:0-0'
		  WHEN ld.OP_DPD >= 1 AND ld.OP_DPD <= 30 THEN 'b:1-30'
		  WHEN ld.OP_DPD >= 31 AND ld.OP_DPD <= 60 THEN 'c:31-60'
		  WHEN ld.OP_DPD >= 61 AND ld.OP_DPD <= 90 THEN 'd:61-90'
		  WHEN ld.OP_DPD >= 91 AND ld.OP_DPD <= 120 THEN 'e:61-90'
		  WHEN ld.OP_DPD >= 121 AND ld.OP_DPD <= 180 THEN 'f:91-120'
		  WHEN ld.OP_DPD > 180 THEN 'g:180'
		END AS COLLX_BKT,
		NULL AS BUCKET_HEAD,
		CASE 
		  WHEN ld.OP_DPD != 0 THEN 'Opening Allocation'
		  WHEN ld.OP_DPD = 0 AND DAY(TRY_TO_DATE(cd.LAST_EMI_DATE, 'YYYY-MM-DD')) = 5 THEN '5th Cycle'
		  WHEN ld.OP_DPD = 0 AND DAY(TRY_TO_DATE(cd.LAST_EMI_DATE, 'YYYY-MM-DD')) = 10 THEN '10th Cycle'
		  WHEN ld.OP_DPD = 0 AND DAY(TRY_TO_DATE(cd.LAST_EMI_DATE, 'YYYY-MM-DD')) = 15 THEN '15th Cycle'
		  ELSE 'NA'
		END AS Cycle,
		CASE 
		  WHEN ld.OP_DPD = 0 THEN 'a:0-0'
		  WHEN ld.OP_DPD >= 1 AND ld.OP_DPD <= 30 THEN 'b:1-30'
		  WHEN ld.OP_DPD >= 31 AND ld.OP_DPD <= 60 THEN 'c:31-60'
		  WHEN ld.OP_DPD >= 61 AND ld.OP_DPD <= 90 THEN 'd:61-90'
		  WHEN ld.OP_DPD >= 91 AND ld.OP_DPD <= 120 THEN 'e:61-90'
		  WHEN ld.OP_DPD >= 121 AND ld.OP_DPD <= 180 THEN 'f:91-120'
		  WHEN ld.OP_DPD > 180 THEN 'g:180'
		END AS CURRENT_DPD_SCALE,
		CASE 
		  WHEN npa_check.CUST_TAG_STATUS = 'linked loan NPA' THEN 'linked loan NPA'
		  WHEN npa_check.CUST_TAG_STATUS = 'NPA' THEN 'NPA'
		  ELSE CURRENT_DPD_SCALE
		END AS OP_DPD_SCALE,  --- to be written
		CASE 
		  WHEN ld.OP_DPD >= 91 AND cd.DPD < 91 AND cd.DPD > 0 THEN 'RTD'
		  WHEN cd.DPD = 0 THEN 'NORMAL'
		  WHEN cd.DPD = ld.OP_DPD THEN 'STABLE'
		  WHEN cd.DPD < ld.OP_DPD THEN 'ROLLBACK'
		  ELSE 'ROLLFORWARD'
		END AS CURRENT_STATUS,
		CASE WHEN CURRENT_STATUS = 'ROLLFORWARD' and TRY_TO_NUMBER(cd.CHEQUE_IN_TRANSIT_COUNT) <> 0 then 'Cheque in transit'
		ELSE CURRENT_STATUS END as STATUS,
		CASE WHEN STATUS IN ('ROLLFORWARD','Cheque in transit') then 'Unpaid'
		ELSE 'Paid' END AS PAID_STATUS,
		CASE WHEN (ld.OP_DPD >= 91 AND cd.DPD > ld.OP_DPD) THEN 'YES' ELSE '' END AS RF_CASES,
		TRY_TO_DATE(cd.EXTRACTED_DATE, 'YYYY-MM-DD') AS EXTRACTED_DATE,
		DATEADD(DAY,-1,TRY_TO_DATE(cd.EXTRACTED_DATE, 'YYYY-MM-DD')) AS BUSINESS_DATE,
	--	TRY_TO_NUMBER(cd.CURRENT_MONTH_INSTLA),
	--	TRY_TO_DATE(cd.CURRENT_MONTH_INSTLAMENT_DUE_DATE, 'YYYY-MM-DD'),
	--	cd.LOAN_AMOUNT,
	--	TRY_TO_DATE(cd.LAST_RECEIVED_DATE_KNOCKOFF_DATE,'YYYY-MM-DD'),
		--cd.LAST_RECEIPT_AMOUNT,
	--	TRY_TO_DATE(cd.FIRST_EMI_DATE,'YYYY-MM-DD'),
	--	TRY_TO_DATE(cd.LAST_EMI_DATE,'YYYY-MM-DD'),
		CASE 
		  WHEN DAY(TRY_TO_DATE(cd.LAST_EMI_DATE,'YYYY-MM-DD')) = 5 THEN 5 
		  WHEN DAY(TRY_TO_DATE(cd.LAST_EMI_DATE,'YYYY-MM-DD')) = 10 THEN 10 
		  WHEN DAY(TRY_TO_DATE(cd.LAST_EMI_DATE,'YYYY-MM-DD')) = 15 THEN 15 
		END AS CYCLE_DATE,
		CASE 
		WHEN laae.POOL_NAME IS NULL then 'ON-BOOK'
		WHEN laae.POOL_NAME like '%PTC%' then 'PTC'
		ELSE 'OFF-BOOK' 
		END AS ON_OFF_BOOK,
    --	NULL AS REPO_FLAG, -- take from duelist
	--	cd.OVERDUE_PRIN,
	--	cd.OVERDUE_INTEREST,
		'EPI' AS EMI_PATTERN,
	--	TRY_TO_DATE(cd.BOOKING_DATE,'YYYY-MM-DD'),
	--	cd.CHEQUE_IN_TRANSIT_AMT,
	--	TRY_TO_NUMBER(cd.CHEQUE_IN_TRANSIT_COUNT),
		bhcax.AMT_COLL_VALUE,
		cd.DISBURSAL_STATUS,
		cd.LOAN_STATUS,
	--	cd.LOAN_PRODUCT,
	--	cd.LOAN_EFF_RATE,
		CASE WHEN cd.DISBURSAL_STATUS = 'FULLY DISBURSED' THEN 'EMI' ELSE 'PEMI' END AS EMI_PEMI,    
		lac.repayment_mode__c AS INSTALLMENT_MODE, 
		lad.LOAN_SANCTION_DATE,
		lad.LAST_DISBURSAL_DATE,
		MAX(cd.TOTAL_NO_OF_INSTL - cd.TOTAL_BILLED_INSTL_NO) AS NET_LTV,
	--	NULL AS NPA_STAGEID, --- can be found from collection legal NPA Stages
	--	cd.EMI_FREQUENCY,
		(cd.TOTAL_DUE + cd.PRE_EMI) AS EMI_OVERDUE_AMOUNT,
		ro_check.EMPLOYEE_ID AS ROID,
		-- NULL AS RO_NAME,
	--	NULL AS BOM_BKT_LM, --- OP DPD 
	--	NULL AS ALLOCATION_LLM, -- redundant
	--	NULL AS ALLOCATION, --
	--	NULL AS ALLOCATION_DATE,
	--	NULL AS DAY1_ALLOCATION, 
	--	NULL AS ALLOCATION_CHANNEL,
	--	NULL AS AGENCY_CODE,
	--	NULL AS NPA_STAGEID_NEW -- redundant
	FROM DISHA_L2_CURATED.HISTORICAL_LOAD.COLL_DUELIST_HISTORY cd
	LEFT JOIN DISHA_L2_CURATED.BASE_MODEL_DATA.LOAN_ACCOUNT_DETAILS lad ON trim(CAST(cd.LOAN_NO AS VARCHAR)) = trim(lad.LOAN_NO)
LEFT JOIN (
    SELECT 
        lad.CUST_ID_FLEX,
        COUNT(DISTINCT cd.LOAN_NO) AS loan_count
    FROM DISHA_L2_CURATED.HISTORICAL_LOAD.COLL_DUELIST_HISTORY cd
    LEFT JOIN DISHA_L2_CURATED.BASE_MODEL_DATA.LOAN_ACCOUNT_DETAILS lad 
        ON trim(CAST(cd.LOAN_NO AS VARCHAR)) = trim(lad.LOAN_NO)
    GROUP BY lad.CUST_ID_FLEX
	) cust_loan_check ON TRIM(lad.CUST_ID_FLEX) = TRIM(cust_loan_check.CUST_ID_FLEX)
	LEFT JOIN (
		SELECT 
			CAST(LOAN_NO AS VARCHAR) AS LOAN_NO,
			EXTRACT(YEAR FROM TRY_TO_DATE(EXTRACTED_DATE, 'YYYY-MM-DD')) AS DPD_YEAR,
			DPD AS MARCH_XX_DPD
		FROM DISHA_L2_CURATED.HISTORICAL_LOAD.COLL_DUELIST_HISTORY
		WHERE TRY_TO_DATE(EXTRACTED_DATE, 'YYYY-MM-DD') = TO_DATE(CONCAT(EXTRACT(YEAR FROM TRY_TO_DATE(EXTRACTED_DATE, 'YYYY-MM-DD')) || '-03-31'),'YYYY-MM-DD')
	) march_dpd
	ON trim(CAST(cd.LOAN_NO AS VARCHAR)) = trim(march_dpd.LOAN_NO) AND EXTRACT(YEAR FROM TRY_TO_DATE(EXTRACTED_DATE, 'YYYY-MM-DD')) = march_dpd.DPD_YEAR
	LEFT JOIN (
		SELECT CAST(cd.LOAN_NO AS VARCHAR) AS LOAN_NO,
			   LAST_DAY(TRY_TO_DATE(EXTRACTED_DATE, 'YYYY-MM-DD')) AS month_end,
			   FIRST_VALUE(DPD) OVER (
				   PARTITION BY LOAN_NO, YEAR(TRY_TO_DATE(EXTRACTED_DATE, 'YYYY-MM-DD')), MONTH(TRY_TO_DATE(EXTRACTED_DATE, 'YYYY-MM-DD'))
				   ORDER BY EXTRACTED_DATE DESC
			   ) AS OP_DPD
		FROM DISHA_L2_CURATED.HISTORICAl_LOAD.coll_duelist_history cd
	) ld ON trim(CAST(cd.LOAN_NO AS VARCHAR)) = trim(ld.LOAN_NO)
	LEFT JOIN (
  SELECT 
    lad.CUST_ID_FLEX,
    CAST(cd.LOAN_NO AS VARCHAR) AS LOAN_NO,
    MAX(CASE WHEN cd.DPD >= 91 THEN 1 ELSE 0 END) OVER (PARTITION BY lad.CUST_ID_FLEX) AS IS_NPA_CUST,
    MAX(CASE WHEN cd.DPD = 0 THEN 1 ELSE 0 END) OVER (PARTITION BY lad.CUST_ID_FLEX) AS HAS_ZERO_DPD,
    CASE 
      WHEN MAX(CASE WHEN cd.DPD >= 91 THEN 1 ELSE 0 END) OVER (PARTITION BY lad.CUST_ID_FLEX) = 1 
           AND MAX(CASE WHEN cd.DPD = 0 THEN 1 ELSE 0 END) OVER (PARTITION BY lad.CUST_ID_FLEX) = 1 THEN 'linked loan NPA'
      WHEN MAX(CASE WHEN cd.DPD >= 91 THEN 1 ELSE 0 END) OVER (PARTITION BY lad.CUST_ID_FLEX) = 1 THEN 'NPA'
      ELSE 'NORMAL'
    END AS CUST_TAG_STATUS
  FROM DISHA_L2_CURATED.HISTORICAl_LOAD.coll_duelist_history cd
  LEFT JOIN DISHA_L2_CURATED.BASE_MODEL_DATA.LOAN_ACCOUNT_DETAILS lad ON trim(CAST(cd.LOAN_NO AS VARCHAR)) = TRIM(lad.LOAN_NO)
  WHERE cd.EXTRACTED_DATE = LAST_DAY(TRY_TO_DATE(EXTRACTED_DATE, 'YYYY-MM-DD'))
) npa_check 
  ON trim(CAST(cd.LOAN_NO AS VARCHAR)) = trim(npa_check.LOAN_NO)	
  LEFT JOIN (select EMPLOYEE_ID, loan_no from disha_l2_curated.base_model_data.loan_account_details LD where ACTIVATION_FLAG = 1)
left join (select * from disha_l2_curated.base_model_data.loan_application_details_trans where loan_application_idv is not null and activation_flag=1) LA
on trim(LA.loan_application_idv)=trim(LD.loan_no)
LEFT JOIN (SELECT * FROM DISHA_L2_CURATED.BASE_MODEL_DATA.EMPLOYEE_DETAILS WHERE ACTIVATION_FLAG=1) E
ON CAST(E.UNIQUE_SYS_ID AS VARCHAR) =CAST(LA.RO_ID AS VARCHAR) where LD.activation_flag=1
) ro_check ON 	trim(CAST(cd.LOAN_NO AS VARCHAR)) = trim(ro_check.loan_no)
	LEFT JOIN DISHA_L1_HARMONIZED.FLEX_LMS.LN_ACCT_ATTRIBUTES_EXT laae ON trim(CAST(cd.LOAN_NO AS VARCHAR)) = trim(laae.COD_ACCT_NO)
	LEFT JOIN DISHA_L1_HARMONIZED.FLEX_LMS.LN_ACCT_DTLS laad ON trim(CAST(cd.LOAN_NO AS VARCHAR)) = trim(laad.cod_acct_no)
	LEFT JOIN DISHA_L1_HARMONIZED.FLEX_LMS.BA_HO_COLL_ACCT_XREF bhcax ON trim(CAST(cd.LOAN_NO AS VARCHAR)) = trim(bhcax.COD_ACCT_NO)
	left JOIN DISHA_L1_HARMONIZED.SFDC_LOS.loan_application__c lac ON trim(CAST(cd.LOAN_NO AS VARCHAR)) = trim(lac.loan_application_id__c)
	--LEFT JOIN DISHA_L1_HARMONIZED.FLEX_LMS.LN_X_REPAY_REGISTER lxrr ON cd.LOAN_NO = trim(lxrr.COD_ACCT_NOcoll
	--WHERE cd.DPD > 0 AND ld.OP_DPD != 0
	where TRY_TO_DATE(EXTRACTED_DATE, 'YYYY-MM-DD') IN  ('2025-03-01','2025-02-28') 
	--AND MONTH(TRY_TO_DATE(EXTRACTED_DATE, 'YYYY-MM-DD')) = '3'  
	GROUP BY 
		TRIM(cd.LOAN_NO), TRIM(cd.LOAN_BRANCH), TRIM(cd.SCHEME), TRIM(lad.CUST_ID_FLEX),cust_loan_check.loan_count,
		cd.TOTAL_NO_OF_INSTL, cd.MORATORIUM, cd.TOTAL_BILLED_INSTL_NO, cd.TOTAL_RECEIV_INSTL,
		cd.OVERDUE_INSTL_COUNT, cd.OVERDUE_PRE_EMI_COUNT, cd.TOTAL_DUE, cd.LPP, cd.PRE_EMI,
		cd.CBC_DUE, cd.OTHER_CHARGES_DUE,laad.FLG_ACCR_STATUS, cd.TOTAL_POS, cd.DPD, ld.OP_DPD,npa_check.CUST_TAG_STATUS,march_dpd.MARCH_XX_DPD,
		cd.CURRENT_MONTH_INSTLA, cd.CURRENT_MONTH_INSTLAMENT_DUE_DATE,
		cd.LOAN_AMOUNT, cd.LAST_RECEIVED_DATE_KNOCKOFF_DATE, cd.LAST_RECEIPT_AMOUNT,
		cd.FIRST_EMI_DATE, cd.LAST_EMI_DATE, laae.POOL_NAME,cd.OVERDUE_PRIN, cd.OVERDUE_INTEREST,
		cd.BOOKING_DATE, cd.CHEQUE_IN_TRANSIT_AMT, cd.CHEQUE_IN_TRANSIT_COUNT, bhcax.AMT_COLL_VALUE,
		cd.DISBURSAL_STATUS, cd.LOAN_STATUS, cd.LOAN_PRODUCT, cd.LOAN_EFF_RATE,lac.repayment_mode__c,
		--lxrr.COD_REPAY_MODE,
		cd.EXTRACTED_DATE,lac.repayment_mode__c, lad.LOAN_SANCTION_DATE, lad.LAST_DISBURSAL_DATE,
		cd.EMI_FREQUENCY, ro_check.EMPLOYEE_ID
